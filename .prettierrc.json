{
  "singleQuote": true,
  "semi": true,
  "importOrder": ["^@(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "printWidth": 110,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "endOfLine": "crlf",
  "proseWrap": "always"
}
